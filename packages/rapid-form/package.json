{
  "name": "rapid-form",
  "version": "3.0.3",
  "private": false,
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "description": "The best way to make a rapid form with react hooks",
  "repository": "git@github.com:Randagio13/rapid-form.git",
  "author": "Alessandro Casazza <imasterale@gmail.com>",
  "license": "MIT",
  "keywords": [
    "rapid-form",
    "react",
    "hooks",
    "form",
    "forms",
    "form-validation",
    "validation",
    "typescript",
    "react-hooks"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "vitest run",
    "test:dev": "vitest",
